<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="in.com.prestige.dto">
	<class name="EmployeeDTO" table="employee_department">
		<id name="employeeId" column="EMPLOYEEID"></id>
		<property name="firsName" column="FIRST_NAME" />
		<property name="lastName" column="LAST_NAME"/>
		<property name="birthDate" column="JOINING_DATE" type="date"/>
		<property name="fordepartkey" column="DEPART_ID"/>

	</class>
</hibernate-mapping>
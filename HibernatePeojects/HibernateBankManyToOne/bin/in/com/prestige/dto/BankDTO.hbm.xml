<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="in.com.prestige.dto">
	<class name="BankDTO" table="Bank_many_to_one">
		<id name="bankId" column="BANKID"></id>
		<property name="bankName" column="BANK_NAME" />
		<property name="address" column="ADRESS" />
	</class>
</hibernate-mapping>
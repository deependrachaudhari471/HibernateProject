<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="in.com.prestige.dto">
	<class name="StockDTO" table="stock_many_to_one">
		<id name="stockID" column="STOCKID"></id>
		<property name="stock_code" column="STOCK_CODE" />
		<property name="stock_name" column="STOCK_NAME" />
		
	</class>
</hibernate-mapping>